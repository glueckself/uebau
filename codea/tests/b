b = fun x -> x.x end;
b1 = fun x -> x.x.x end;
b2 = fun x -> x.4.x end;
b3 = fun x -> x.4.(x+1).5 end;

%%.0
const sListElem list[] = {
    {.type = ELEM_NUM, .head=6, .tail=6},
    {.type = ELEM_END}
};

LIST_EQUAL(b(6), list);

%%.0
const sListElem list[] = {
    {.type = ELEM_NUM, .head=6},
    {.type = ELEM_NUM, .head=TAG_NUM(4) .tail=6},
    {.type = ELEM_END}
};

LIST_EQUAL(b2(6), list);
